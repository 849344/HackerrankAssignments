import java.io.*;
import java.math.*;
import java.security.*;
import java.text.*;
import java.util.*;
import java.util.concurrent.*;
import java.util.regex.*;

public class Solution {

    // Complete the superDigit function below.
    private static int  sumOfDigits(int number){
        int sum=0;
        while(number>0){
            sum+=(number%10);
            number/=10;
        }
        return sum;
    }
    private static int super_digit(int number){
        //int num=Integer.parseIntnumber);
        if(number<10)//Single digit
        return number; 
        else
        {
            int res= sumOfDigits(number);
            return super_digit(res);
        }
    } 
    
    static int superDigit(String n, int k){
        int len=n.length();
        int new_n=0;
        for(int i=0;i<len;i++){
            new_n=new_n+(Integer.parseInt(n.charAt(i)+"") * k);
        }
        return super_digit(new_n);
    }

    private static final Scanner scanner = new Scanner(System.in);

    public static void main(String[] args) throws IOException {
        BufferedWriter bufferedWriter = new BufferedWriter(new FileWriter(System.getenv("OUTPUT_PATH")));

        String[] nk = scanner.nextLine().split(" ");

        String n = nk[0];

        int k = Integer.parseInt(nk[1]);

        int result = superDigit(n, k);

        bufferedWriter.write(String.valueOf(result));
        bufferedWriter.newLine();

        bufferedWriter.close();

        scanner.close();
    }
}
